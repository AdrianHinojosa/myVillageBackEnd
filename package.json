{
  "name": "shipoBack",
  "version": "1.0.0",
  "description": "Shipo Backend",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --transpile-only src/server.ts",
    "build": "babel src --out-dir dist --extensions \".ts\" --copy-files",
    "db:rollback": "cd src && knex migrate:rollback",
    "db:migrations": "cd src && knex migrate:latest",
    "db:seeds": "cd src && knex seed:run",
    "migrate-script": "ts-node-dev --transpile-only migrationScript.ts",
    "update-prod-migrations": "NODE_ENV=production ts-node-dev --transpile-only productionMigrationUpdate.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SofexTech/shipoBack.git"
  },
  "keywords": [
    "Shipo",
    "node",
    "express",
    "typescript",
    "postgres"
  ],
  "author": "Sofex Technologies",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SofexTech/shipoBack/issues"
  },
  "homepage": "https://github.com/SofexTech/shipoBack#readme",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.454.0",
    "aws-sdk": "^2.740.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^11.0.2",
    "core-js": "^2.6.12",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^8.2.0",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-fileupload": "^1.2.1",
    "handlebars": "^4.7.7",
    "helmet": "^3.21.2",
    "joi": "^17.4.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "knex-postgis": "0.14.0",
    "mailcomposer": "^4.0.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.0",
    "objection": "^3.0.1",
    "pg": "^8.8.0",
    "secure-random-password": "^0.2.3",
    "serialize-error": "^5.0.0",
    "sharp": "^0.32.0",
    "strip-comments": "^2.0.1",
    "stripe": "^11.3.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.26.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/node-cron": "^3.0.1",
    "@types/sharp": "^0.29.5",
    "@types/supertest": "^2.0.11",
    "jest": "^27.4.7",
    "nodemon": "^2.0.4",
    "supertest": "^6.2.0",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
